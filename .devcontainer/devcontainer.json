{
  "name": "hardhat.solidity-reentrancy-workshop.workshops.utkusarioglu.com",
  "dockerComposeFile": [
    "../.docker/docker-compose.dev.yml",
    "docker-compose.yml"
  ],
  "service": "solidity-reentrancy-workshop-hardhat",
  "workspaceFolder": "/utkusarioglu-com/workshops/solidity-reentrancy-workshop/hardhat",
  "postCreateCommand": "scripts/post-create-command.sh",
  "customizations": {
    "vscode": {
      "settings": {
        "workbench.colorCustomizations": {
          "activityBar.background": "#dd4400",
          "activityBar.foreground": "#FFFFFF"
        }
      },
      "extensions": [
        "JuanBlanco.solidity",
        "MythX.mythxvsc",
        "joaompinto.vscode-graphviz",
        "cschleiden.vscode-github-actions",
        "GitHub.vscode-pull-request-github"
      ]
    }
  }
}
